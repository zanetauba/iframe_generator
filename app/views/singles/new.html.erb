<div class="container">
  <div class="white-wrapper">
    <%= simple_form_for @single do |f| %>
      <div class="form-inputs">
        <%= f.input :element, required: true %>
        <%= f.input :iframe, required: true %>
      </div>
      <div class="form-actions">
        <%= f.button :submit, class: 'btn-submit-single' %>
      </div>
    <% end %>
  </div>
</div>
