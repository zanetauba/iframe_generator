<%= simple_form_for @triple do |f| %>
  <div class="form-inputs">
    <%= f.input :element %>
    <%= f.input :iframe1 %>
    <%= f.input :iframe2 %>
    <%= f.input :iframe3 %>
    <%= f.input :comment %>
  </div>
  <div class="form-actions">
    <%= f.button :submit, class: 'btn btn-primary' %>
  </div>
<% end %>
